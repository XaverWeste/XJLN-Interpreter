def bool = true | false => obj

    def !=(bool value, bool b) :: bool = !(value == b)

    def !(bool value) :: bool
        if value -> return false
        else -> return true
    end

    def &(bool value, bool b) : bool
        if !value -> return = false
        else -> return = b
    end

    def |(bool value, bool b) : bool
        if value -> return = true
        else -> return = b
    end
end